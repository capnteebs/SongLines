# Last.fm API
# Get your key at: https://www.last.fm/api/account/create
VITE_LASTFM_API_KEY=your_lastfm_api_key_here

# Spotify API (optional)
# Get credentials at: https://developer.spotify.com/dashboard
VITE_SPOTIFY_CLIENT_ID=your_spotify_client_id_here
VITE_SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here

# Deezer (optional - no key needed for basic access)
VITE_DEEZER_APP_ID=your_deezer_app_id_here

# Apple Music (optional - requires Apple Developer Program)
VITE_APPLE_MUSIC_DEVELOPER_TOKEN=your_apple_developer_token_here

# Discogs API (optional - fallback for credits when MusicBrainz is sparse)
# Get your token at: https://www.discogs.com/settings/developers
VITE_DISCOGS_TOKEN=your_discogs_token_here
